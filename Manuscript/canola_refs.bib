@Proceedings{Mesquida1988,
  title     = {A preliminary study of insect pollination of male-sterile oil-seed rape (\emph{Brassica napus}) in `mixed' cultures with variable proportions of pollinising plants},
  year      = {1988},
  abstract  = {Promising performances of winter and spring oilseed rape have drawn attention to the interest of F1 hybrid varieties (Lefort-Buson & Datte 1985; Sernyk 1983). Amongst different sources of male cytoplasmic sterility currently under study (CMS, Rousselle 1981) is a CMS derived from radish (Ogura 1968; Bannerot et al 1983), improved by protoplast fusion (cybrids obtained by Pelletier et al 1983). However, because of difficulties encountered in selection of restorer lines (Pellan-Delourme 1986) the possibility of exploiting male sterility without fertility restoration in the hybrid F1 is being considered. One way of doing this is to use a `mixed' variety, in which F1 hybrid male-steriles obtained from cybrids mentioned above are exploited as `females', with male-fertile pollinisers, in a given proportion. Yield in such a mixture would be assured by the hybrid F1, the 'male' plants serving as pollen source. However, by-passing the problem of fertility restoration in cybrids in this way raises fresh problems. These make it necessary to examine the significance of factors affecting pollination, notably the role of bees, and of those involved in competition, in order to determine the type of male-fertile oilseed to be used, and the most appropriate proporation of fertile plants in the mixtures. As a first step work has begun using a same line of spring ra, in its male-fertile (`male') and male-sterile (`female') forms. Results of tests undertaken in 1985 are described here.},
  author    = {Mesquida, J. and Renard, M.},
  file      = {:C\:\\Users\\Samuel\\Documents\\Papers\\Mesquida and Renard 1988.pdf:PDF},
  groups    = {Canola pollination},
  journal   = {Apidologie},
  owner     = {Samuel},
  pages     = {353--366},
  timestamp = {2014.05.26},
}


@Article{Delbrassine1988,
  author    = {Delbrassine, S. and Rasmont, P.},
  journal   = {Bulletin des recherches agronomiques de {Gembloux}},
  title     = {Contribution \`{a} l'\'{e}tude de la pollinisation du colza, \emph{Brassica napus} {L}. var. \emph{oleifera} ({Moench}) {Delile}, en {Belgique}},
  year      = {1988},
  pages     = {123--152},
  volume    = {23},
  abstract  = {[Translated] The authors studied insect pollinators of canola (Brassica napus L. var oleifera) Delile, in 4 locations of Wallonie (southern Belgium) from 1984 to 1985.

The most important species of pollinators were social Apidae: Apis mellifera, Bombus terrestris, B. lucorum, B. cryptarum, and Pyrobombus lapidarius. Honeybees visit the nectaries of canola using 3 different methods. In the "climbing-over" method, after the first nectary, the bee climbed over the style to visit the second. In the "bypassing" method, after the first nectary, the bee went around the style to get to the second nectary. In the "inserting" method, the bee inserted its tongue from the outside of the corolla between the base of the petals (nectar theiving). Only the first method deposited pollen on the stigma. Bumblebees used only the first method.

In the fields provided with hives, honeybees used the second and third foraging methods most frequently, with consequently low pollination. In a single field without hives, bumblebees were more abundant than honeybees, non-aggresive, and used the "climbing-over" method.

The rates of visitation were: 10.6-14 fls/min (Apis mellifera), 32.3 (B. terrestris), 30.5 (B. lucorum), 31.2 (B. cryptarum), 38.6 (P. lapidarius).

In the fields provided with hives, due to unfavorable pollinating behaviour by honeybees, bumblebees appear to be the best pollinator, due to their speed at foraging, and despite their low relative abundance.},
  comment   = {Translated title: [{Honeybee} contribution to the pollination of oilseed rape (\emph{Brassica napus} {L}. var. \emph{oleifera}) {Delile} in {Belgium}]},
  file      = {:C\:\\Users\\Samuel\\Documents\\Papers\\delbrassine and rasmont 1988.pdf:PDF},
  groups    = {Canola pollination, Honey bees},
  owner     = {Samuel},
  timestamp = {2017.03.22},
}

@Article{Dukas1998,
  author    = {Reuven Dukas and Leah Edelstein-Keshet},
  journal   = {Journal of Theoretical Biology},
  title     = {The spatial distribution of colonial food provisioners},
  year      = {1998},
  issn      = {0022-5193},
  number    = {2},
  pages     = {121--134},
  volume    = {190},
  abstract  = {We present spatially explicit models to predict the optimal distribution of numerous food provisioners that share a nesting aggregation. We compare predictions for solitary and social provisioners, and for three foraging currencies. When distinct food patches with identical food parameters are considered, fewer provisioners use the farther patches. The rate of decline in number of provisioners depends on the currency used. With net rate of energy intake (net energy gain over time), the rate of decline is relatively small under a wide range of realistic parameter values and distances; with efficiency (net energy gain over energy cost) and empirically derived energetic costs, the rate of decline is much higher. With lifetime fitness (lifetime food delivery under predation risk), outcomes depend on the ratio of mortality risk during flight and during food collection at the patch. With all currencies, there is a difference between the optimal spatial distribution of solitary and social provisioners, with a larger proportion of social provisioners being farther from the central place. The optimal distribution of solitary provisioners in a two dimensional field of uniformly distributed food such as a flowering meadow is such that the density of solitary provisioners (number per unit area) declines monotonically with distance, and provisioner number peaks at half the maximum area used.},
  comment   = {Derived expectations for CPF distributions in a 2-patch scenario, where patches were identical except for distance.},
  doi       = {http://dx.doi.org/10.1006/jtbi.1997.0530},
  file      = {:C\:\\Users\\Samuel\\Documents\\Papers\\dukas and edelstein-keshet 1998.pdf:PDF},
  groups    = {Pollinator movement, Foraging theory, Movement},
  owner     = {Samuel},
  timestamp = {2015.09.21},
  url       = {http://www.sciencedirect.com/science/article/pii/S0022519397905300},
}

@Article{Duran2010,
  author    = {Dur{\'a}n, Ximena Araneda and Ulloa, Rodrigo Breve and Carrillo, Jos{\'e} Aguilera and Contreras, Jorge Lav{\'\i}n and Bastidas, Marcelo Toneatti},
  journal   = {Chilean Journal of Agricultural Research},

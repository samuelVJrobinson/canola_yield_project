  groups    = {Post-pollination processes},
  owner     = {Samuel},
  publisher = {JSTOR},
  timestamp = {2015.03.10},
}

@Article{Fijen2018,
  author    = {Thijs P. M. Fijen and Jeroen A. Scheper and Timo M. Boom and Nicole Janssen and Ivo Raemakers and David Kleijn},

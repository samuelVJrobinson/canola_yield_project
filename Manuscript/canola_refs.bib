  publisher = {Wiley},
}

@Article{Manning2005,
  author    = {Manning, Rob and Wallis, Ian R},
  journal   = {Animal Production Science},
  title     = {Seed yields in canola (\emph{Brassica napus} cv. Karoo) depend on the distance of plants from honeybee apiaries},
  year      = {2005},
  number    = {10},
  pages     = {1307--1313},
  volume    = {45},
  abstract  = {This research examined the benefits of placing hives of honeybees (Apis melifera L.) in canola (Brassica napus L.) at a density of approximately 1 hive/ha. We tested 3 main hypotheses. First, deploying honeybees increases the yield of seed. Second, that the benefits of honeybee pollination decline the further plants are from an apiary. Third, poorly pollinated plants should channel more resources into larger seeds, whereas plants benefiting from insect pollination should produce more but smaller seeds. The experiment confirmed all 3 hypotheses. Yields of seeds increased by more than 20% or 400kg/ha, whereas the yield declined in plots located more than 200m from the apiary. There are several explanations for the increased yield, including an enhanced production of fertile pods and, therefore, more seed, particularly small seed.},
  comment   = {Compared yield at distance from 3 bee-yards, as well as a control crop (no bees). Found that plants closer to bee hives make more pods and have a higher yield of seeds (didn't count number of seeds).


Pollination rate was 0.40 hives/acre (1/ha)},

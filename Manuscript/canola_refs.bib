@Article{Mesquida1981,
  author    = {Mesquida, J. and Renard, M.},
  journal   = {Apidologie},
  title     = {Pollinisation du colza d'hiver m\^{a}le-fertile et m\^{a}le-sterile (\emph{Brassica napus} {L}. var \emph{oleifera} Metzger) par l'abeille domestique (\emph{Apis m. mellifica} {L.}): effets sur la ph\'{e}nologie et le rendement.},
  year      = {1981},
  pages     = {345--362},
  volume    = {12},
  abstract  = {Les auteurs ont étudié l’effet de la pollinisation sur quelques caractères phénologiques et sur quelques composantes du rendement du colza d’hiver mâle-fertile (variété « Primor n) et mâle-stérile d’origine cytoplasmique. L’effet du butinage s’est surtout manifesté, sur les plantes mâle-stériles seulement, par une augmentation significative de certains caractères (nombre de fleurs, nouaisons, nombre de siliques, nombre de graines par silique, production et rendement en graines) ainsi que par une diminution significative d’autres caractères (durée de floraison, taille des ramifications primaires, production de fleurs et taille des graines). Ces mêmes effets sont également apparus sur les plantes mâle-fertiles mais plus atténués et non significatifs, sauf en ce qui concerne les nouaisons et la production grainière de 1 000 fleurs. Le rendement en graines des plantes mâle-fertiles n’a finalement augmenté que de 6 %. L’effet dû au vent seul a été non négligeable sur les nouaisons des plantes mâle-stériles mais les rendements ont été au contraire négligeables.


The authors studied the effect of pollination on several phenological and yield characters of male-fertile winter rapeseed. Visitation caused changes in the male-sterile plants only, increasing numbers of flowers, forming fruits, siliques, seeds/pod, yield. It shortened flowering period, reduced primary branch size, flower production, and size of grains. The same effects appeared in male-fertile plants, but were not significant. Yield was increased by 6%. The effect of wind alone was significant on fruit-formation of the male-sterile plants, but the yield was not significantly different.},
  comment   = {Basically, the authors did caged pollination experiments in rapeseed with "mini-hives", and found that while bee pollination increased yield in male-sterile canola, it didn't significantly increase yield in male-fertile canola.

This could be due to:
-smallish sample size (9 plots total, if data from both years are lumped together), but used 5 plants/plot. May not constitute independent samples (mixed models hadn't been invented yet).
-unrealistic bee treatment (lack of a queen changes foraging behaviour / desire to forage)
-varietal differences},
  file      = {:C\:\\Users\\Samuel\\Documents\\Papers\\mesquida and renard 1981.pdf:PDF},
  groups    = {Canola pollination},
  owner     = {Samuel},
  timestamp = {2014.05.26},
}

@Article{Mohr1988,
